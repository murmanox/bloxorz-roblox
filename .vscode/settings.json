{
	"editor.codeActionsOnSave": {
		"source.addMissingImports": true
	},

	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"node_modules/[^@]*": true,
		"node_modules/@[^r]*": true,
		"**.{rbxl,rbxl.lock}": true,
		"include/**": true,
		"out/**": true
	}
}
